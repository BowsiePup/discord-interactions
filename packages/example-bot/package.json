{
  "name": "@discord-interactions/example-bot",
  "description": "An example Discord bot using @discord-interactions/core and fastify.",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "format": "prettier --write src",
    "lint": "eslint src",
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest tests",
    "docs": "npx typedoc",
    "start": "node dist/index.js",
    "dev": "yarn build && yarn start",
    "prepublish": "yarn build && yarn test && yarn docs"
  },
  "dependencies": {
    "@discord-interactions/builders": "^0.0.1",
    "@discord-interactions/core": "^0.0.1",
    "@discord-interactions/verify-node": "^0.0.1",
    "fastify": "^4.2.0",
    "fastify-raw-body": "^4.0.0",
    "redis": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.18.3",
    "@types/node-fetch": "^2.6.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "dotenv": "^13.0.0",
    "jest": "^28.1.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.13",
    "typedoc-plugin-markdown": "^3.13.3",
    "typedoc-plugin-merge-modules": "^3.1.0",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "discord",
    "interactions",
    "slash commands",
    "framework",
    "api",
    "wrapper"
  ]
}
