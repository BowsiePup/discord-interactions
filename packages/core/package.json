    "formdata-node": "^4.3.3",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.18.3",
    "@types/node-fetch": "^2.6.1",
    "@types/uuid": "^8.3.4",
