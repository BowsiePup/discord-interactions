{
  "name": "interactions.ts",
  "description": "A full command framework for Discord interactions based on @discordjs/builders.",
  "version": "1.0.28",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ssMMiles/interactions.ts.git"
  },
  "scripts": {
    "format": "prettier --write src",
    "lint": "eslint src/**/*.ts",
    "build": "yarn format & yarn lint && tsc -p tsconfig.json",
    "test": "mocha -r ts-node/register 'tests/**/*.ts'",
    "docs": "npx typedoc",
    "start": "node dist/index.js",
    "dev": "yarn build && yarn start",
    "prepublish": "yarn build && yarn test && yarn docs"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.32.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/deep-equal": "^1.0.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^14.18.3",
    "@types/node-fetch": "^2.6.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "deep-equal": "^2.0.5",
    "dotenv": "^13.0.0",
    "eslint": "^8.14.0",
    "fastify": "^3.27.4",
    "fastify-raw-body": "^3.2.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.13",
    "typedoc-plugin-merge-modules": "^3.1.0",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist/**/*"
  ],
  "author": "ssMMiles",
  "license": "MIT",
  "keywords": [
    "discord",
    "interactions",
    "slash commands",
    "framework",
    "api",
    "wrapper"
  ]
}
